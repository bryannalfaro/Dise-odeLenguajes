/* definicion de parser */

/* INICIA seccion de tokens */

%token TOKEN_1
%token TOKEN_2
%token TOKEN_3 TOKEN_4
%token WS
IGNORE WS

/* Finaliza seccion de tokens */

%%

/* INICIA seccion de producciones */
production1:
     production1 TOKEN_1 production2
   | production2
;
production2:
     production2 TOKEN_2 production3
   | production3
;
production3:
     TOKEN_3 production1 TOKEN_4
   | TOKEN_1
;
/* Finaliza seccion de producciones */